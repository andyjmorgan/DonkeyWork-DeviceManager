{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext"]
  },
  "AllowedHosts": "*",
  "KeycloakConfiguration": {
    "Authority": "https://auth.donkeywork.dev/realms/DeviceManager",
    "InternalAuthority": "http://keycloak.infrastructure.svc.cluster.local:8080/realms/DeviceManager",
    "ClientId": "DeviceManager",
    "ClientSecret": "",
    "RedirectUri": "https://api.donkeywork.dev/callback",
    "AdminClientId": "DeviceManager",
    "AdminClientSecret": ""
  }
}
